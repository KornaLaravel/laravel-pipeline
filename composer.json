{
    "name": "chefhasteeth/pipeline",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pipeline",
        "pipe"
    ],
    "autoload": {
        "psr-4": {
            "Chefhasteeth\\Pipeline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chefhasteeth\\Pipeline\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Sam Bedwell",
            "email": "hello@chefhasteeth.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.2|^8.0",
        "orchestra/testbench": "^7.5|^9.0",
        "phpunit/phpunit": "^9.5|^10.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
